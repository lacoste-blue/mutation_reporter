        <td><%= mutations_less_so.build %></td>
        <td><%= mutations_less_so.file_name %></td>
        <td><%= mutations_less_so.line_number %></td>
        <td><%= mutations_less_so.diff %></td>
        <td><%= link_to 'Show', mutations_less_so %></td>
